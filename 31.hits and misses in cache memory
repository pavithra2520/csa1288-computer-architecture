#include <stdio.h>
int main() {
    int hits, misses;
    float hit_ratio;
    printf("Enter the number of hits in cache memory: ");
    scanf("%d", &hits);
    printf("Enter the number of misses in cache memory: ");
    scanf("%d", &misses);
    hit_ratio = (float)hits / (hits + misses);
    printf("Hit ratio: %.2f\n", hit_ratio);
    return 0;
}
