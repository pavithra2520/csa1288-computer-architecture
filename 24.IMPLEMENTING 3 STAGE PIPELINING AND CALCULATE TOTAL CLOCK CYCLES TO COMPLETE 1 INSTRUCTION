#include <stdio.h>
int main() {
    int total_instructions;
    int n = 3; 
    printf("Enter the number of instructions: ");
    scanf("%d", &total_instructions);
    int total_cycles = total_instructions * n;
    printf("Total clock cycles needed to complete 1 instruction in a 3-stage pipeline: %d cycles\n", total_cycles);
    return 0;
}
